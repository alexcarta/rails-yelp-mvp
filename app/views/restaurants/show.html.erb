
<div class="d-flex justify-content-center mt-5">
  <div class="card" style="width: 40rem">
    <img src="data:image/jpeg;base64," class="card-img-top">
    <div class="card-body">

      <h5 class="card-title"><%= @restaurant.name %></h5>

      <p class="card-text"><%= @restaurant.category %> couisine</p>

      <p class="card-text">&#128205 <%= @restaurant.address %></p>

      <p class="card-text"> &#128222 <%= @restaurant.phone_number %></p>

      <p> &#128483 Clients said about the <%= @restaurant.name %></p>
      <% @restaurant.reviews.each do |review| %>
        <ul>
          <li>
            <p><%= review.content %></p>
            <p>Rating: <%= review.rating %></p>
          </li>
        </ul>
      <% end %>

      <%= link_to "Review it!", new_restaurant_review_path(@restaurant) %>

    </div>
  </div>
</div>
